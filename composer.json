{
  "name": "masgeek/fuelrod-rest-api-client",
  "description": "Fuelrod rEST api package for messaging and other things",
  "keywords": [
    "SMS",
    "fuelrod",
    "munywele",
    "AfricasTalking"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sammy M",
      "email": "barsamms@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Fuelrod\\": "src/",
      "FuelrodLegacy\\": "src/",
      "Fuelrod\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
